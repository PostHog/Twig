{
  "name": "@posthog/twig",
  "version": "0.0.0-dev",
  "description": "Twig - PostHog desktop task manager",
  "main": ".vite/build/bootstrap.js",
  "versionHash": "dynamic",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  },
  "scripts": {
    "setup": "bash bin/setup",
    "dev": "electron-forge start",
    "start": "electron-forge start",
    "start:debug": "electron-forge start -- --inspect=5858 --remote-debugging-port=9222",
    "package": "electron-forge package",
    "package:dev": "FORCE_DEV_MODE=1 SKIP_NOTARIZE=1 electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "build": "pnpm package",
    "build-native": "bash scripts/build-native-modules.sh",
    "build-icons": "bash scripts/generate-icns.sh",
    "typecheck": "tsc -p tsconfig.node.json --noEmit && tsc -p tsconfig.web.json --noEmit",
    "generate-client": "tsx scripts/update-openapi-client.ts",
    "test": "vitest run",
    "test:e2e": "playwright test --config=tests/e2e/playwright.config.ts",
    "test:e2e:headed": "playwright test --config=tests/e2e/playwright.config.ts --headed",
    "postinstall": "cd ../.. && npx @electron/rebuild -f -m node_modules/node-pty || true && bash apps/twig/scripts/patch-electron-name.sh && node apps/twig/scripts/download-binaries.mjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "posthog",
    "twig",
    "tasks",
    "developer-tools"
  ],
  "author": "PostHog",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
